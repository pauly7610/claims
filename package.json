{
  "name": "@claims/monorepo",
  "version": "1.0.0",
  "description": "AI-Powered Insurance Claims Processing Agent with Full Observability",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test:backend": "python test_backend.py",
    "test:mlops": "python test_mlops.py",
    "mlops:start": "cd ml/mlops && python mlops_service.py",
    "mlflow:ui": "open http://localhost:5000",
    "mlops:api": "open http://localhost:8007/docs",
    "start:all": "python start_services.py",
    "k8s:deploy": "kubectl apply -f k8s/",
    "storybook": "npm run storybook --workspace=@claims/design-system"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "turbo": "^1.10.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a",
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.544.0",
    "react-dropzone": "^14.3.8"
  }
}
